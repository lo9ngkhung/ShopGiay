                                <script>

                                    function selectSuggest(act) {

                                        cur = $('.smart-search-wrapper > .select').index();

                                        length = $('.smart-search-wrapper > a').length;

                                        if (act == 38) {

                                            if (cur == -1 || cur == 0)

                                                cur = length - 1;

                                            else

                                                cur = cur - 1;

                                        }


                                        if (act == 40) {

                                            if (cur == -1 || cur == length - 1)

                                                cur = 0;

                                            else

                                                cur = cur + 1;
                                        }

